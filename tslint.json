{
    "extends": [
        "tslint-config-standard"
    ],
    "rules": {
        "no-unused-variable": false,
        "ter-indent": [
            true,
            4
        ],
        "object-curly-spacing": [
            true,
            "always"
        ],
        "one-variable-per-declaration": false,
        "import/extensions": [
            "error",
            "always",
            {
                "ts": "never"
            }]
    }
}
