{
  "watchDirs": {
    "model": {
      "watch": "register",
      "directory": "src/app/model",
      "enabled": true,
      "typings": "{cwd}",
      "generator": "auto",
      "interfaceHandle": "InstanceType<{{ 0 }}>"
    },
    "config": {
      "watch": "register",
      "interface": "Config",
      "directory": "src/config",
      "enabled": true,
      "typings": "{cwd}",
      "generator": "auto",
      "interfaceHandle": "InstanceType<{{0}}>"
    }
  }
}